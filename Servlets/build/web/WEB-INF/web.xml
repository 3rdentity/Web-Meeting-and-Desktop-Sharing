<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Connect</servlet-name>
        <servlet-class>servlets.Connect</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetFile</servlet-name>
        <servlet-class>servlets.GetFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetFileList</servlet-name>
        <servlet-class>servlets.GetFileList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetSS</servlet-name>
        <servlet-class>servlets.GetSS</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportKeyboard</servlet-name>
        <servlet-class>servlets.ReportKeyboard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReportMouse</servlet-name>
        <servlet-class>servlets.ReportMouse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendMessage</servlet-name>
        <servlet-class>servlets.SendMessage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RequestOTP</servlet-name>
        <servlet-class>servlets.RequestOTP</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AuthorizeOTP</servlet-name>
        <servlet-class>servlets.AuthorizeOTP</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Connect</servlet-name>
        <url-pattern>/Connect</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetFile</servlet-name>
        <url-pattern>/GetFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetFileList</servlet-name>
        <url-pattern>/GetFileList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetSS</servlet-name>
        <url-pattern>/GetSS</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportKeyboard</servlet-name>
        <url-pattern>/ReportKeyboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReportMouse</servlet-name>
        <url-pattern>/ReportMouse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendMessage</servlet-name>
        <url-pattern>/SendMessage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestOTP</servlet-name>
        <url-pattern>/RequestOTP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AuthorizeOTP</servlet-name>
        <url-pattern>/AuthorizeOTP</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
